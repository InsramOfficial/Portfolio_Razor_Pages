@page
@model Iportfolio.Pages.myadmin.AddtestimonialModel
@{
}

<div class="container">
    <form method="post" enctype="multipart/form-data">
        <div class="row justify-content-center">
            <h2> ADD Testamonial</h2>
        </div>
        <div class="row justify-content-center">
            <div class=" col-md-6">
                <label class="form-label">Name </label>
                <input class="form-control" asp-for="testimonial.Name" />
            </div>
        </div>
        <div class="row justify-content-center">
            <div class=" col-md-6">
                <label class="form-label">Designation </label>
                <input class="form-control" asp-for="testimonial.Designation" />
            </div>
        </div>
        <div class="row justify-content-center">
            <div class=" col-md-6">
                <label class="form-label">TheTestimonial </label>
                <input class="form-control" asp-for="testimonial.TheTestimonial" />
            </div>
        </div>
        <div class="row justify-content-center">
            <div class=" col-md-6">
                <label class="form-label">Image </label>
                <input class="form-control" asp-for="testimonial.Photo" />
            </div>
        </div>
       

        <div class="row justify-content-center">
            <div class=" col-md-6">

                <input type="submit" value="Add Testamonial" class="btn btn-primary" />
            </div>
        </div>

    </form>
</div>